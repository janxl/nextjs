{   "version":2,
    "name":"nextjs",
    "alias": ["squealingpig.rhm.net.au", "ativo.rhm.net.au"],
    "builds":[
        {"src":"package.json","use":"@now/next"}
    ],
    "routes":[
        {"src":"/favicon.ico","dst":"/favicon.ico"},
        {"src":"/_next/(.*)$","dst":"/_next/$1"},
        {"src":"/static/(.*)$","dst":"/static/$1"},
        {"src":"/(.*)$","dest":"/index?page=$1"}
    ]
}