{   "version":2,
    "name":"nextjs",
    "alias": ["squealingpig.rhm.net.au", "ativo.rhm.net.au"],
    "builds":[
        {"src":"package.json","use":"@now/next"}
    ],
    "routes":[
        {"src":"/_next/.*","continue":true},
        {"src":"/styles/.*","continue":true},
        {"src":"/favicon.ico","continue":true},
        {"src":"/(.*)$","dest":"/index?page=$1"}
    ]
}